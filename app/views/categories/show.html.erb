<div class="category-jumbo" style="height: 82px; margin-top: -20px">
            <%= form_tag(category_path, :method => "get") do %>
                <div class="search-form" style='margin-top: -10px; margin-left: -1%; width: 100%!important'>
                    <%= text_field_tag :search, params[:search], placeholder: "Start searching for your tee....", class: "form-control  search-1" %>
                    <%= button_tag "", type: 'submit', class: "btn btn-default glyphicon glyphicon-search" %>
                </div>
            <% end %>
</div>
<% if @category_subcategories.present? %>
	<%= render 'subcategories/sub', obj: @category_subcategories %>
<% end %>
	<h2  class="product-list-header">NEW PRODUCTS</h2>
<%= render 'listings/listing', obj: @category_listings %>